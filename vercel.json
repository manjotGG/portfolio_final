{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/mainwebsite.html"
    },
    {
      "src": "/scribbling",
      "dest": "/scribbling.html"
    },
    {
      "src": "/graphic-design",
      "dest": "/graphic_design.html"
    },
    {
      "src": "/illustrator",
      "dest": "/Illustrator.html"
    },
    {
      "src": "/about",
      "dest": "/about-me.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
